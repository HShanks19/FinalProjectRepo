pipeline{
        agent any
        tools {
		    maven 'M3'
		  }
        stages{
            stage('Build'){
                steps{
                	sh '''
                        cd FinalProjectCitizenRepo
                        mvn clean install
                    '''
                }
            }
            stage('Deploy') { 
                steps {
                    sh 'java -jar /home/jenkins/.m2/repository/com/qa/FinalProjectCitizenRepo/0.0.1-SNAPSHOT/FinalProjectCitizenRepo-0.0.1-SNAPSHOT.jar -d'
                }
            }
        }
}