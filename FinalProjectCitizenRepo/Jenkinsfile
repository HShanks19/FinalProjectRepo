pipeline{
        agent any
        tools {
		    maven 'M3'
		  }
        stages{
            stage('Build'){
                steps{
                	sh "cd FinalProjectCitizenRepo"
                    sh "mvn clean package"
                }
            }
            stage('Deploy') { 
                steps {
                    sh 'java -jar target/finalprojectcitizenrepo-0.0.1-SNAPSHOT.jar'
                }
            }
        }
}